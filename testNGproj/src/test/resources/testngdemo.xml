<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DemoSuite">
	<test name="Login opration">
		<classes>
			<class name="test.test.testNgdemo1">
				<methods>
					<include name="testing" />
					<include name="credentials" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="navigate to myinfo">
